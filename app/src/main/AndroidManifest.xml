<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.deepak.bangloreguid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BanglorePalace"
            android:label="@string/palace" />
        <activity
            android:name=".FoodStreet"
            android:label="@string/food" />
        <activity
            android:name=".Lalbag"
            android:label="@string/garden" />
        <activity
            android:name=".ZoologicalPark"
            android:label="@string/zoo"></activity>
    </application>

</manifest>